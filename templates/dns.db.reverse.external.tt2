[%- USE DBI( dbh = dbh ) -%]
$ttl 1d
[% IF (matches = zone.match('10\..*')) -%]
@ IN  SOA dns.cs.brown.edu. root.cs.brown.edu. ( [% serial_num %] 10800 3600 604800 86400 )
  IN  NS  dns.cs.brown.edu.
[% ELSE -%]
@ IN  SOA dns.cs.brown.edu. root.cs.brown.edu. ( [% serial_num %] 10800 3600 604800 86400 )
  IN  NS  dns.cs.brown.edu.
  IN  NS  bru-ns1.brown.edu.
  IN  NS  bru-ns2.brown.edu.
  IN  NS  ns1.ucsb.edu.
[% END -%]

